<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<Version>1.0.1.0</Version>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <OutputType>Library</OutputType>
    <Title>$(AssemblyName)</Title>
    <Company>Laurin Zehnder</Company>
    <Authors>Firestorm253</Authors>
    <Copyright>Laurin Zehnder</Copyright>
    <PackageId>FireMath.NET</PackageId>
    <PackageProjectUrl>https://github.com/Firestorm-253/Math.NET</PackageProjectUrl>
    <PackageOutputPath>bin\Debug\packages</PackageOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MathNet.Numerics" Version="5.0.0" />
  </ItemGroup>

</Project>
